{
  "name": "celltrak-poc",
  "description": "Electron based application for proof-of-concept purposes",
  "version": "1.0.0",
  "author": "Joan Lascano",
  "dependencies": {
    "@types/express": "^4.0.37",
    "@types/material-ui": "^0.20.2",
    "@types/node": "^8.0.33",
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/react-helmet": "^5.0.3",
    "@types/react-router-dom": "^4.0.8",
    "@types/react-swipeable-views": "^0.12.0",
    "express": "^4.16.1",
    "express-static-gzip": "^0.3.0",
    "material-ui": "^0.20.0",
    "react": "^16.0.0",
    "react-async-bootstrapper": "^1.1.1",
    "react-async-component": "^1.0.1",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "react-swipeable-views": "^0.12.12",
    "electron": "^1.4.10",
    "webpack": "^3.6.0",
    "process.argv": "^0.1.0",
    "ws": "^1.1.1",
    "electron-packager": "^8.3.0",
    "jshint": "^2.9.4",
    "@types/benchmark": "^1.0.30",
    "@types/webpack-env": "^1.13.1",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "benchmark": "^2.1.4",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "source-map-loader": "^0.2.2",
    "typescript": "^2.5.3",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-stats-plugin": "^0.1.5"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "electron-dmg": "hdiutil create -srcfolder build/mac/celltrak-poc-darwin-x64/ build/mac/celltrak-poc-darwin-x64.dmg",
    "electron-packager-mac": "./node_modules/.bin/electron-packager . --overwrite --platform=darwin --arch=x64 --out=build/mac --ignore=tmp/ --ignore=build/mac --ignore=build/win32 --ignore=test/ --ignore=.idea/ --electron-version=$(./node_modules/.bin/electron -v | cut -c 2-)",
    "electron-packager-win32": "./node_modules/.bin/electron-packager . --overwrite --platform=win32 --arch=x64 --out=build/win32 --ignore=tmp/ --ignore=build/mac --ignore=build/win32 --ignore=test/ --ignore=.idea/ --electron-version=$(./node_modules/.bin/electron -v | cut -c 2-)",
    "build-benchmark": "webpack --progress --config ./config/webpack.benchmark.config.js",
    "benchmark": "node ./build/benchmark/benchmark.js",
    "build-server": "webpack --progress --config ./config/webpack.server.config.js",
    "build-client": "webpack --progress --config ./config/webpack.client.config.js",
    "build:web": "npm run build-server && npm run build-client",
    "build:mac": "npm run build:web && npm run electron-packager-mac && npm run electron-dmg",
    "build:windows": "build:web && electron-packager-win32",
    "build-serve": "npm run build:web && npm run serve",
    "serve": "node ./build/web/server/server.js",
    "start-electron": "npm run build:web && ./node_modules/.bin/electron ./main.js"
  }
}
